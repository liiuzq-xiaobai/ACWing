/*
数据量小，暴力枚举L范围内的所有情况，即枚举分子分母
*/
#include<iostream>
#include<cstring>
#include<algorithm>

using namespace std;

int L;
int A,B;

int main(){
    int resA,resB;
    double delta=1e9;;//差值
    
    cin>>A>>B>>L;
    for(int a=0;a<=L;a++){
        for(int b=0;b<=L;b++){
            double x=(double)a/b;
            double X=(double)A/B;
            if(x>=X&&x-X<delta){
                delta=x-X;
                resA=a;
                resB=b;
            }
        }
    }
    cout<<resA<<" "<<resB<<endl;
    return 0;
}
