//偏移量法来控制走步方向
//扩展：844题 flood fill算法 1102题 1421题
#include <iostream>
#include <cstring>
#include <algorithm>
#include <sstream>
using namespace std;
const int MAX=105;
int n,m;
int w[MAX][MAX];
int main(){
    cin>>n;
    cin>>m;
    //定义偏移量
    int dx[]={-1,0,1,0};
    int dy[]={0,1,0,-1};
    //定义初始位置
    int x=0,y=0;
    int direct=1;//初始方向
    for(int i=1;i<=n*m;i++){
        w[x][y]=i;
        int a=x+dx[direct];
        int b=y+dy[direct];
        if(a<0||a>=n||b<0||b>=m||w[a][b]){//碰墙
            //换方向
            direct=(direct+1)%4;
            a=x+dx[direct];
            b=y+dy[direct];
        }
        x=a;
        y=b;
    }
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            cout<<w[i][j]<<" ";
        }
        cout<<endl;
    }
    return 0;
}
