/*
    DP 递归方程：v[i][j]=max
*/
#include <iostream>
#include <cstring>
#include <algorithm>
#include <sstream>

using namespace std;
const int MAX=1000;
int n;
int a[MAX][MAX];
int main(){
    cin>>n;
    for(int i=1;i<=n;i++){
        for(int j=1;j<=i;j++){
            cin>>a[i][j];
        }
    }
    int res;
    cout<<res<<endl;
}
